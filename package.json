{
  "name": "serverless-boilerplate-express-typescript",
  "version": "1.0.0",
  "description": "Serverless framework ExpressJS TypeScript",
  "scripts": {
    "dev": "cross-env NODE_ENV=development sls offline start --stage offline",
    "tail-log": "sls logs -f app -t",
    "deploy-prod": "cross-env NODE_ENV=production sls deploy --stage prod",
    "remove-prod": "cross-env NODE_ENV=production sls remove --stage prod",
    "clean": "rimraf dist .webpack .serverless",
    "lint": "eslint --ext .js,.ts .",
    "build-types": "tsc --noEmit --pretty",
    "test": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "aws-sdk": "^2.1125.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.0.2",
    "nanoid": "^3.3.3",
    "nodemailer": "^6.7.4",
    "serverless-http": "^3.0.1",
    "stripe": "^8.219.0",
    "ulid": "^2.3.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@nearst/serverless-dynalite": "^2.0.4",
    "@types/aws-lambda": "^8.10.95",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest-dynalite": "^3.5.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "serverless": "^3.16.0",
    "serverless-bundle": "^5.3.0",
    "serverless-dotenv-plugin": "^4.0.0",
    "serverless-offline": "^8.7.0",
    "serverless-offline-ses": "^0.0.7",
    "typescript": "^4.6.4"
  },
  "author": "Ixartz (https://github.com/ixartz)"
}
