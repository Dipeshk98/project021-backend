{
  "name": "serverless-boilerplate-express-typescript",
  "version": "1.0.0",
  "description": "Serverless framework ExpressJS TypeScript",
  "scripts": {
    "dev": "cross-env NODE_ENV=development sls offline start --stage offline",
    "tail-log": "sls logs -f app -t",
    "deploy-prod": "cross-env NODE_ENV=production sls deploy --stage prod",
    "remove-prod": "cross-env NODE_ENV=production sls remove --stage prod",
    "clean": "rimraf dist .webpack .serverless",
    "lint": "eslint --ext .js,.ts .",
    "build-types": "tsc --noEmit --pretty",
    "prepare": "husky install"
  },
  "dependencies": {
    "aws-sdk": "^2.1045.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "serverless-http": "^2.7.0",
    "stripe": "^8.192.0",
    "ulid": "^2.3.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "serverless": "^2.68.0",
    "serverless-bundle": "^5.2.0",
    "serverless-dotenv-plugin": "^3.10.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^8.3.1",
    "typescript": "^4.4.2"
  },
  "author": "Ixartz (https://github.com/ixartz)"
}
