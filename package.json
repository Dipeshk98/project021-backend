{
  "name": "serverless-boilerplate-express-typescript",
  "version": "1.0.0",
  "description": "Serverless framework ExpressJS TypeScript",
  "scripts": {
    "dev:db": "ts-node scripts/database.ts",
    "dev:server": "cross-env NODE_ENV=development sls offline --noAuth --host 0.0.0.0",
    "dev": "run-p dev:*",
    "tail-log": "sls logs -f app -t",
    "deploy:db": "dotenv -e .env.production -c -- ts-node --esm scripts/deployDbProd.ts",
    "deploy:app": "cross-env NODE_ENV=production sls deploy --stage prod",
    "deploy-prod": "run-s deploy:*",
    "remove-prod": "cross-env NODE_ENV=production sls remove --stage prod",
    "clean": "rimraf dist .webpack .serverless",
    "lint": "eslint .",
    "format": "eslint --fix . && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty",
    "test": "jest",
    "test:dev": "start-server-and-test dev http://localhost:4000 'curl --head http://localhost:4000'",
    "db:push": "prisma db push",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.328.0",
    "@prisma/client": "^4.13.0",
    "aws-jwt-verify": "^5.0.0",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.4",
    "bson": "^4.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.7",
    "jwks-rsa": "^3.2.0",
    "mongodb": "^6.12.0",
    "multer": "^1.4.5-lts.2",
    "nanoid": "^5.0.9",
    "nodemailer": "^6.9.1",
    "pino": "^8.12.1",
    "pino-lambda": "^4.2.1",
    "serverless-http": "^3.2.0",
    "stripe": "^11.18.0",
    "ulid": "^2.3.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.56",
    "@swc/jest": "^0.2.26",
    "@types/aws-lambda": "^8.10.115",
    "@types/cors": "^2.8.13",
    "@types/cross-spawn": "^6.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "esbuild": "^0.16.17",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "execa": "^7.1.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "mongodb-memory-server": "^8.12.2",
    "node-mocks-http": "^1.12.2",
    "npm-run-all": "^4.1.5",
    "p-retry": "^4.6.2",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "rimraf": "^5.0.0",
    "serverless": "^3.30.1",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.44.0",
    "serverless-offline": "^14.4.0",
    "serverless-offline-ses-v2": "^1.0.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "author": "safe space labs"
}
