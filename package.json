    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.1.1",
    "nanoid": "^3.3.4",
    "nodemailer": "^6.8.0",
    "pino": "^8.6.1",
    "pino-lambda": "^4.1.0",
    "serverless-http": "^3.0.3",
    "stripe": "^10.14.0",
    "ulid": "^2.3.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@nearst/serverless-dynalite": "^2.0.4",
